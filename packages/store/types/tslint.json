{
  "extends": "dtslint/dtslint.json",
  "rules": {
    "semicolon": false,
    "indent": [false, "tabs"],
    "no-useless-files": false,
    "eofline": false,
    "no-unnecessary-class": false,
    "member-access": false,
    "no-object-literal-type-assertion": false,
    "interface-over-type-literal": false,
    "whitespace": [
      false,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "radix": false,
    "prefer-object-spread": false,
    "no-empty-interface": false
  }
}
